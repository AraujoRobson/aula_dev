package atividade1;

public class Contas {

	public static void main(String[] args) {
		
		ContaPoupanca poup = new ContaPoupanca("Robson", 1516);
		
		
		
		//POUPANÇA
		poup.depositar(50000);
		poup.sacar(1000);
		poup.setDiaRendimento(50);
		System.out.println(poup.toString());
		System.out.println("-----------");
		
		//ESPECIAL
		

	}

}
