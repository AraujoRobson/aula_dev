package Exercicio1;

public class Testes {

	public static void main(String[] args) {
		
		Assistente assist = new Assistente("Teste assist", 1000, 154);
		AssistenteTecnico assistTec = new AssistenteTecnico("Assist Técnico", 2000, 100, 100);
		AssistenciaAdm assistAdm = new AssistenciaAdm("ADM", 1500, 2, 500);
		
		
		assist.exibeDados();
		System.out.println("--------------");
		assistTec.exibeDados();
		System.out.println("--------------");
		assistAdm.exibeDados();
	}

}
