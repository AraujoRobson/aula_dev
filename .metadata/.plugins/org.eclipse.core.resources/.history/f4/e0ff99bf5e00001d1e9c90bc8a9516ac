package atividade2;

public class Mamifero extends Animal {

	public Mamifero(String nome, String cor, String alimento, float comprimento, float velocidade) {
		super(nome, cor, alimento, comprimento, velocidade);
	}

	@Override
	public void alteraNome(String nome) {
		setNome(nome);		
	}

	@Override
	public void alteraComprimento(float comprimento) {
		setComprimento(comprimento);	
	}

	@Override
	public void alteraCor(String cor) {
		setCor(cor);
	}

	@Override
	public void alteraAmbiente(String ambiente) {
		setAmbiente(ambiente);
	}

	@Override
	public void alteraVelocidade(float velocidade) {
		setVelocidade(velocidade);
	}
	
	

}
