package atividade2;

public class Peixe extends Animal{

	public Peixe(String nome, String caracteristica, float comprimento, float velocidade) {
		super(nome, caracteristica,comprimento, velocidade);
	}

	

	public void alteraCaracteristica(float caracteristica) {
		setVelocidade(caracteristica);
	}



	@Override
	public String toString() {
		return super.toString() + 
				"Caracteristica: " + getCaracteristica();
	}
	
	
	
	

}
