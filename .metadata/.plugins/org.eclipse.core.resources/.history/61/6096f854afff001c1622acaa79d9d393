package Exercicio3;

public class Principal {

	public static void main(String[] args) {
		Fornecedor fornecedor = new Fornecedor("ABC LTDA", "Rua Goiabinha", "49 7070 7070");
		Empregado empregado = new Empregado("Fulado da Silva Santos", "Av Faria Lima", "49 1020 3040", 5);
		
		//FORNECEDOR
		fornecedor.setValorCredito(1000);
		fornecedor.setValorDivida(300);
		System.out.println("Fornecedor.");
		System.out.println(fornecedor.toString());
		System.out.println("-------");
		
		//EMPREGADO
		empregado.setSalarioBase(2000);
		empregado.setImposto(179);
		System.out.println("Empregado.");
		System.out.println(empregado.toString());
		System.out.println("-------");
		
	}

}
